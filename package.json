{
  "name": "aframe-room-component",
  "version": "0.5.0",
  "description": "An A-Frame component for quickly creating rooms connected by doors.",
  "main": "./index.js",
  "cdn": "./dist/aframe-room-component.min.js",
  "engines": {
    "node": ">=18.13.0",
    "yarn": ">=1.22.0"
  },
  "scripts": {
    "build": "webpack index.js dist/aframe-room-component.js && webpack -p index.js dist/aframe-room-component.min.js",
    "lint": "semistandard -v | snazzy",
    "start": "budo index.js:dist/aframe-room-component.min.js --port 7000 --live --open"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oparamo/aframe-room-component.git"
  },
  "keywords": [
    "aframe",
    "aframe-component",
    "aframe-vr",
    "vr",
    "mozvr",
    "webvr",
    "room"
  ],
  "contributors": [
    {
      "name": "Hazel Fraticelli",
      "email": "omgitsraven@gmail.com"
    },
    {
      "name": "Oscar Paramo",
      "email": "oscar@paramo.dev"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/oparamo/aframe-room-component/issues"
  },
  "homepage": "https://github.com/oparamo/aframe-room-component#readme",
  "devDependencies": {
    "@babel/core": "7.20.12",
    "@babel/preset-env": "7.20.2",
    "aframe": "1.4.1",
    "babel-loader": "9.1.2",
    "browserify": "17.0.0",
    "budo": "11.8.4",
    "semistandard": "16.0.1",
    "snazzy": "9.0.0",
    "webpack": "5.75.0",
    "webpack-cli": "5.0.1"
  },
  "semistandard": {
    "globals": [
      "AFRAME",
      "THREE"
    ],
    "ignore": [
      "examples/build.js",
      "dist/**"
    ]
  }
}
