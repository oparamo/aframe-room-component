{
  "name": "aframe-room-component",
  "version": "0.5.0",
  "description": "An A-Frame component for quickly creating rooms connected by doors.",
  "main": "./index.js",
  "cdn": "./dist/aframe-room-component.min.js",
  "engines": {
    "node": "^18.13.0",
    "yarn": "^1.22.0"
  },
  "scripts": {
    "dev": "budo index.js:dist/aframe-room-component.min.js --port 7000 --live --open",
    "dist": "webpack index.js dist/aframe-room-component.js && webpack -p index.js dist/aframe-room-component.min.js",
    "lint": "semistandard -v | snazzy",
    "prepublish": "npm run dist",
    "ghpages": "ghpages",
    "start": "npm run dev",
    "test": "karma start ./tests/karma.conf.js",
    "test:firefox": "karma start ./tests/karma.conf.js --browsers Firefox",
    "test:chrome": "karma start ./tests/karma.conf.js --browsers Chrome"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oparamo/aframe-room-component.git"
  },
  "keywords": [
    "aframe",
    "aframe-component",
    "aframe-vr",
    "vr",
    "mozvr",
    "webvr",
    "room"
  ],
  "contributors": [
    {
      "name": "Hazel Fraticelli",
      "email": "omgitsraven@gmail.com"
    },
    {
      "name": "Oscar Paramo",
      "email": "oscar@paramo.dev"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/oparamo/aframe-room-component/issues"
  },
  "homepage": "https://github.com/oparamo/aframe-room-component#readme",
  "devDependencies": {
    "@babel/core": "7.20.12",
    "@babel/preset-env": "7.20.2",
    "aframe": "1.4.1",
    "babel-loader": "9.1.2",
    "browserify": "17.0.0",
    "budo": "11.8.4",
    "chai": "4.3.7",
    "chai-shallow-deep-equal": "1.4.6",
    "ghpages": "0.0.10",
    "karma": "6.4.1",
    "karma-browserify": "8.1.0",
    "karma-chai-shallow-deep-equal": "0.0.4",
    "karma-chrome-launcher": "3.1.1",
    "karma-env-preprocessor": "0.1.1",
    "karma-firefox-launcher": "2.1.2",
    "karma-mocha": "2.0.1",
    "karma-mocha-reporter": "2.2.5",
    "karma-sinon-chai": "2.0.2",
    "mocha": "10.2.0",
    "randomcolor": "0.6.2",
    "semistandard": "16.0.1",
    "shelljs": "0.8.5",
    "shx": "0.3.4",
    "sinon": "15.0.1",
    "sinon-chai": "3.7.0",
    "snazzy": "9.0.0",
    "webpack": "5.75.0",
    "webpack-cli": "5.0.1"
  },
  "semistandard": {
    "globals": [
      "AFRAME",
      "THREE"
    ],
    "ignore": [
      "examples/build.js",
      "dist/**"
    ]
  }
}
